<div class="container">
  <div class="page-header">
    <h1><%= @company.name %></h1>
  </div>
  <table class="table table-striped table-hover">
    </thead>
      <tr>
        <th>Products</th>
        <th>Slogan</th>
      </tr>
    </thead>
    <tbody>
      <% @company.products.each do |product| %>
        <tr>
          <td><%= link_to product.name, product_path(product)  %></td>
          <td><%= @company.catch_phrase  %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
